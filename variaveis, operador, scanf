#include <stdio.h>

int main (){
    int base;
    int altura;
    int area;
    printf("Digite um valor para a base:\n");
    scanf("%i", &base);

    printf("Digite um valor para a altura:\n");
    scanf("%i", &altura);

    area = base * altura;
    printf("a area e = %i\n", area);
    return 0;
}
